<h1>食品リスト</h1>
<ul class="food_list">
  <% @foods.each do |food| %>
    <li class="food_list_item <%= food.food_category.name_en %>">
      <%= link_to food.name, storages_path(storage:
        { food_id: food.id, user_id: current_user.id }), method: :post %>
    </li>
  <% end %>
</ul>
<%= link_to '食材新規登録', new_food_path %>
